<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveController</servlet-name>
  	<display-name>SaveController</display-name>
  	<description></description>
  	<servlet-class>com.controller.SaveController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteController</servlet-name>
  	<display-name>DeleteController</display-name>
  	<description></description>
  	<servlet-class>com.controller.DeleteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindController</servlet-name>
  	<display-name>FindController</display-name>
  	<description></description>
  	<servlet-class>com.controller.FindController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UploadController</servlet-name>
  	<display-name>UploadController</display-name>
  	<description></description>
  	<servlet-class>com.controller.UploadController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveController</servlet-name>
  	<url-pattern>/SaveController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteController</servlet-name>
  	<url-pattern>/DeleteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindController</servlet-name>
  	<url-pattern>/FindController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UploadController</servlet-name>
  	<url-pattern>/UploadController</url-pattern>
  </servlet-mapping>
</web-app>
